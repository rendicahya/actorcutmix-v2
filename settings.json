{
  "active": {
    "dataset": "ucf101",
    "random_seed": 0,
    "use_REPP": true,
    "detector": "yolov8-coco"
  },
  "datasets": {
    "ucf101": {
      "path": "data/ucf101/videos",
      "ext": ".avi",
      "class_index": "data/ucf101/annotations/classInd.txt",
      "n_videos": 13320,
      "n_classes": 101
    },
    "hmdb51": {
      "path": "data/hmdb51/videos",
      "ext": ".avi",
      "split": 1,
      "n_videos": 6766,
      "n_classes": 51,
      "frames": "data/hmdb51/frames"
    },
    "kinetics100": {
      "path": "data/kinetics100/videos",
      "ext": ".mp4",
      "split": 1,
      "n_videos": 9999,
      "n_classes": 100
    }
  },
  "detect": {
    "generate_videos": true,
    "yolov8-coco": {
      "checkpoint": "checkpoints/yolov8x.pt",
      "confidence": 0.25,
      "human_class": [0]
    },
    "yolov8-iov7": {
      "checkpoint": "checkpoints/yolov8x-iov7.pt",
      "confidence": 0.25
    },
    "yolov10-coco": {
      "checkpoint": "checkpoints/yolov10x.pt",
      "confidence": 0.25,
      "human_class": [0]
    }
  },
  "cutmix": {
    "smooth_edge": false,
    "multiplication": 10,
    "scene_replace": "black",
    "output": {
      "ext": ".mp4",
      "writer": "moviepy"
    }
  }
}
